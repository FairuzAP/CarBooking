    <bpmn:serviceTask id="ServiceTask_1sxs2q3" name="Get Transaction Details">
      <bpmn:extensionElements>
        <camunda:connector>
          <camunda:inputOutput>
            <camunda:inputParameter name="url">
              <camunda:script scriptFormat="javascript"><![CDATA[var base = "http://localhost:2222/api/rest/trans/";
var value = execution.getVariable("Transaction_ID");
var url = base + value;
url;]]></camunda:script>
            </camunda:inputParameter>
            <camunda:inputParameter name="method">GET</camunda:inputParameter>
            <camunda:inputParameter name="headers">
              <camunda:map>
                <camunda:entry key="Accept">application/json</camunda:entry>
              </camunda:map>
            </camunda:inputParameter>
            <camunda:outputParameter name="transaction">
              <camunda:script scriptFormat="javascript">S(response);</camunda:script>
            </camunda:outputParameter>
            <camunda:outputParameter name="isSuccess">
              <camunda:script scriptFormat="javascript"><![CDATA[var resp = S(response);
resp.prop("status") == 2]]></camunda:script>
            </camunda:outputParameter>
            <camunda:outputParameter name="finish_date">
              <camunda:script scriptFormat="javascript"><![CDATA[var resp = S(response);
resp.prop("waktu_selesai");]]></camunda:script>
            </camunda:outputParameter>
          </camunda:inputOutput>
          <camunda:connectorId>http-connector</camunda:connectorId>
        </camunda:connector>
      </bpmn:extensionElements>
      <bpmn:incoming>SequenceFlow_1lqa663</bpmn:incoming>
      <bpmn:outgoing>SequenceFlow_0v3hywp</bpmn:outgoing>
