            <camunda:inputParameter name="payload">
              <camunda:script scriptFormat="javascript"><![CDATA[var date = execution.getVariable("finish_date") + 24*60*60*execution.getVariable("Days");
date;]]></camunda:script>
            </camunda:inputParameter>
          </camunda:inputOutput>
          <camunda:connectorId>http-connector</camunda:connectorId>
        </camunda:connector>
